{
  "name": "prototype-icon-management",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "paths": {
    "babelOutput": "./dist/cjs",
    "babelSource": "./src"
  },
  "ports": {
    "dev": 9000
  },
  "scripts": {
    "build:app": "cross-env MODE=production webpack",
    "build:dist": "node ./bin/prepDist && node ./bin/buildIconsManifest && npm run transpile && npm run sync:css",
    "start": "npm run build:dist && cross-env NODE_ENV=production npm run start:server",
    "start:dev": "npm run build:dist && cross-env DEBUG=true PORT=$npm_package_ports_dev concurrently --kill-others -p {name} -n CSS,BABEL,WEBPACK,SERVER -c black.bgMagenta,black.bgGreen,black.bgCyan,black.bgMagenta \"npm run sync:css -- -w -v -u\" \"npm run watch:transpile\" \"npm run watch:app\" \"npm run watch:server\"",
    "start:server": "node $npm_package_paths_babelOutput/server",
    "sync:css": "cpx \"src/server/views/**/*.css\" dist/cjs/server/views",
    "transpile": "babel $npm_package_paths_babelSource -d $npm_package_paths_babelOutput --verbose",
    "watch:app": "cross-env MODE=development webpack --watch",
    "watch:server": "babel-node ./src/server/watcher",
    "watch:transpile": "npm run transpile -- --skip-initial-build --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the0neWhoKnocks/prototype.icon-management"
  },
  "author": "",
  "dependencies": {
    "cpx": "^1.5.0",
    "mkdirp": "^0.5.1",
    "request": "^2.88.0",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@noxx/babel-plugin-a2rp": "^1.0.0",
    "@noxx/eslint-config": "^1.0.7",
    "@noxx/webpack-tidy-plugin": "^2.0.2",
    "acorn": "^6.1.1",
    "ajv": "^5.5.2",
    "babel-loader": "^8.0.5",
    "babel-plugin-emotion": "^10.0.7",
    "babel-plugin-transform-define": "^1.3.1",
    "browser-sync": "^2.26.5",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "emotion": "^10.0.7",
    "fs-extra": "^7.0.1",
    "nodemon": "^1.18.10",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "simple-progress-webpack-plugin": "^1.1.2",
    "webpack": "^4.24.0",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.1.2"
  }
}
